{
  "name": "beam-trace",
  "version": "3.0.0",
  "description": "2D and 3D acoustic beam tracing with BSP tree acceleration, fail plane caching, and skip sphere optimization",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./2d": {
      "import": "./dist/beamtrace2d.js",
      "types": "./dist/beamtrace2d.d.ts"
    },
    "./3d": {
      "import": "./dist/beamtrace3d.js",
      "types": "./dist/beamtrace3d.d.ts"
    }
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/index.d.ts.map",
    "dist/beamtrace2d.js",
    "dist/beamtrace2d.d.ts",
    "dist/beamtrace2d.d.ts.map",
    "dist/beamtrace3d.js",
    "dist/beamtrace3d.d.ts",
    "dist/beamtrace3d.d.ts.map",
    "dist/core",
    "dist/geometry",
    "dist/geometry.js",
    "dist/geometry.d.ts",
    "dist/geometry.d.ts.map",
    "dist/optimization.js",
    "dist/optimization.d.ts",
    "dist/optimization.d.ts.map",
    "dist/structures",
    "dist/optimization",
    "dist/solver",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && npm run bundle",
    "bundle": "node build.mjs",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "start": "node server.js",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kai5z/BeamTrace2D.git"
  },
  "keywords": [
    "acoustics",
    "beam tracing",
    "ray tracing",
    "room acoustics",
    "audio",
    "3d",
    "bsp",
    "typescript"
  ],
  "author": "Kai Saksela",
  "contributors": [
    "Akos Kemives <akoskm@gmail.com> (http://akoskm.com/)"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/kai5z/BeamTrace2D/issues"
  },
  "homepage": "https://github.com/kai5z/BeamTrace2D#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/qunit": "^2.19.10",
    "@types/three": "^0.181.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.1",
    "express": "^4.14.0",
    "qunitjs": "^2.0.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.53.3"
  }
}
